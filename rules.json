{
  "rules": [
    {
      "predicate": "all",
      "conditions": [
        {
          "field": "subject",
          "contains": "This"
        },
        {
          "field": "sender",
          "contains": "info@members.tatastarbucks.net"
        }
      ],
      "actions": [
        {
          "action": "mark_as_read"
        },
        {
          "action": "move",
          "folder": "Important/Urgent"
        }
      ]
    },
    {
      "predicate": "any",
      "conditions": [
        {
          "field": "subject",
          "contains": "Laziness"
        },
        {
          "field": "subject",
          "contains": "schedule"
        }
      ],
      "actions": [
        {
          "action": "mark_as_read"
        },
        {
          "action": "move",
          "folder": "Meetings"
        }
      ]
    },
    {
      "predicate": "all",
      "conditions": [
        {
          "field": "sender",
          "contains": "notifications@github.com"
        }
      ],
      "actions": [
        {
          "action": "move",
          "folder": "GitHub"
        }
      ]
    },
    {
      "predicate": "all",
      "conditions": [
        {
          "field": "date_received",
          "predicate": "less_than",
          "value": "7"
        }
      ],
      "actions": [
        {
          "action": "move",
          "folder": "Old Emails"
        }
      ]
    },
    {
      "predicate": "any",
      "conditions": [
        {
          "field": "date_received",
          "predicate": "greater_than",
          "value": "1"
        },
        {
          "field": "subject",
          "contains": "Urgent"
        }
      ],
      "actions": [
        {
          "action": "mark_as_read"
        },
        {
          "action": "move",
          "folder": "Immediate Attention"
        }
      ]
    }
  ]
}
